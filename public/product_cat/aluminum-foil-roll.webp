<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>
  
  <!-- Aluminum foil roll -->
  <g transform="translate(200,150)">
    <!-- Roll cylinder -->
    <ellipse cx="0" cy="0" rx="80" ry="25" fill="#e9ecef" stroke="#6c757d" stroke-width="2"/>
    <rect x="-80" y="-25" width="160" height="50" fill="#dee2e6"/>
    <ellipse cx="0" cy="25" rx="80" ry="25" fill="#ced4da" stroke="#6c757d" stroke-width="2"/>
    
    <!-- Foil texture lines -->
    <g stroke="#adb5bd" stroke-width="1" opacity="0.7">
      <line x1="-70" y1="-20" x2="-70" y2="20"/>
      <line x1="-50" y1="-22" x2="-50" y2="22"/>
      <line x1="-30" y1="-23" x2="-30" y2="23"/>
      <line x1="-10" y1="-24" x2="-10" y2="24"/>
      <line x1="10" y1="-24" x2="10" y2="24"/>
      <line x1="30" y1="-23" x2="30" y2="23"/>
      <line x1="50" y1="-22" x2="50" y2="22"/>
      <line x1="70" y1="-20" x2="70" y2="20"/>
    </g>
    
    <!-- Unrolled foil sheet -->
    <path d="M 80 0 Q 120 -30 160 -20 Q 180 -15 190 10 Q 185 40 160 45 Q 120 50 80 25" 
          fill="#f8f9fa" stroke="#6c757d" stroke-width="2" opacity="0.9"/>
    
    <!-- Foil shine effect -->
    <path d="M 85 5 Q 115 -20 145 -15 Q 160 -12 165 8" 
          fill="none" stroke="#ffffff" stroke-width="3" opacity="0.6"/>
    
    <!-- Core tube -->
    <ellipse cx="0" cy="0" rx="15" ry="8" fill="#6c757d"/>
    <rect x="-15" y="-8" width="30" height="16" fill="#495057"/>
    <ellipse cx="0" cy="8" rx="15" ry="8" fill="#343a40"/>
  </g>
  
  <!-- Title -->
  <text x="200" y="50" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#212529">
    Aluminum Foil Roll
  </text>
  
  <!-- Decorative elements -->
  <circle cx="50" cy="80" r="3" fill="#6c757d" opacity="0.3"/>
  <circle cx="350" cy="220" r="4" fill="#6c757d" opacity="0.3"/>
  <circle cx="80" cy="250" r="2" fill="#6c757d" opacity="0.3"/>
</svg>